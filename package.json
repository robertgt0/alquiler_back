{
  "name": "alquiler_back",
  "version": "1.0.0",
  "description": "API Alquiler - lista para Render",
  "type": "commonjs",
  "engines": { "node": ">=18" },
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "seed": "ts-node-dev --transpile-only src/seed/seedOffers.ts",
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "start:dev": "npm run build && npm start",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robertgt0/alquiler_back.git"
  },
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "mongoose": "^8.19.1",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@types/connect": "^3.4.38",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/mongoose": "^5.11.96",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.8.0",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
