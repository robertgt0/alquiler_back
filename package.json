{
  "name": "alquiler_back",
  "version": "1.0.0",
  "description": "Backend de alquiler con TypeScript, Express y Mongoose",
  "type": "commonjs",
  "engines": {
    "node": ">=18"
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "seed": "ts-node-dev --transpile-only src/seed/seedOffers.ts",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "start:dev": "npm run build && npm start",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robertgt0/alquiler_back.git"
  },
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@google-cloud/pubsub": "^5.2.0",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^10.0.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "googleapis": "^161.0.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.0",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.9",
    "rimraf": "^6.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/connect": "^3.4.38",
    "@types/express": "^5.0.3",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.8.0",
    "nodemon": "^3.1.10",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  }
}
